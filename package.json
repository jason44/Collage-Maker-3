{
  "name": "Collage-Maker-3",
  "version": "1.0.0",
  "type": "module",
  "description": "A simple program to view images in a collage.",
  "main": "main.js",
  "scripts": {
    "start": "electron . --gtk-version=3 --ozone-platform=wayland",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.jason44.CollageMaker3",
    "productName": "Collage Maker 3",
    "mac": {
      "category": "public.app-category.graphics-design",
      "icon": "images/appicon.png"
    },
    "win": {
      "icon": "images/appicon.png"
    },
    "linux": {
      "icon": "images/appicon.png"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jason44/Collage-Maker-3.git"
  },
  "keywords": [
    "image preview",
    "collage"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jason44/Collage-Maker-3/issues"
  },
  "homepage": "https://github.com/jason44/Collage-Maker-3#readme",
  "devDependencies": {
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "electron-store": "^10.1.0"
  }
}
